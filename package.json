{
  "name": "manulife-genie-ionic-angular-core",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "del-cli dist && npm run transpile && npm run package-tsc && npm run package-fonts && npm run package-images && npm run package-scss && npm run clean-up",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "transpile": "ngc",
    "package-tsc": "cpx \"dist/out-tsc/src/**/*.*\" \"dist\"",
    "package-fonts": "cpx \"src/assets/cmp-ui-theme/fonts/*.*\" \"dist/assets/cmp-ui-theme/fonts\"",
    "package-images": "cpx \"src/assets/cmp-ui-theme/images/*.*\" \"dist/assets/cmp-ui-theme/images\"",
    "package-scss": "cpx \"src/assets/cmp-ui-theme/**/*.scss\" \"dist/assets/cmp-ui-theme\"",
    "clean-up": "del-cli \"dist/out-tsc\" | del-cli \"dist/node_modules\"",
    "package": "del-cli dist && cpx \"src/assets/**/*.*\" \"dist/assets/\" && cpx \"src/index.ts\" \"dist/\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19",
    "ionic-angular": "3.9.2",
    "cpx": "1.5.0",
    "ngx-device-detector": "1.3.5",
    "moment": "2.22.2",
    "@angular/material": "5.2.5",
    "@angular/cdk": "5.2.5",
    "@ngx-translate/core": "9.1.1",
    "@ngx-translate/http-loader": "2.0.1",
    "manulife-mobile-ionic-angular-ui": "git+https://git.ap.manulife.com/scm/cmp/cmp-ionic-angular-theme.git#0.1.4"
  },
  "devDependencies": {
    "@angular/cli": "~1.7.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "del-cli": "^1.1.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  }
}
